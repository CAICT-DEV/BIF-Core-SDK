        <maven.compiler.target>8</maven.compiler.target>
    </properties>

    <dependencies>
        <dependency>
            <groupId>cn.bitfactory</groupId>
            <artifactId>bif-chain-sdk</artifactId>
            <version>1.0.3.3</version>
        </dependency>
        <dependency>
